[app]

# Application name
name = My App

# Application version
version = 0.1

# Application package
package.name = myapp

# Dependencies
requirements = python3,kivy

# Android permissions
android.permissions = CAMERA

# Android API level
android.api = 28

# Target Android ABI
android.arch = armeabi-v7a

# Build type
android.build_type = release

# Buildozer version
buildozer.version = 1.2